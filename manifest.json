{
    "manifest_version": 3,
    "name": "Hide TikTok Comments",
    "version": "1.0",
    "description": "Automatically hides comments section on TikTok video pages.",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["*://*.tiktok.com/*"],
        "js": ["contentScript.js"],
        "run_at": "document_end"
      }
    ]
  }
  